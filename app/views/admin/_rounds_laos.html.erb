<%= form_with(model: rounds, local: true, class: "form") do |form| %>

<div class="input-group input-has-value">
    <label class="col-md-3 col-form-label" for="l10">งวดวันที่ :</label>
  <input type="text" class="form-control datepicker" placeholder="งวดวันที่">
  <span class="input-group-addon">
    <i class="list-icon material-icons">date_range</i>
  </span>
</div>

<div class="form-group mr-b-40 input-has-value">

    <label class="col-md-3 col-form-label" for="l10">วันเวลาที่เปิดรับแทง :</label>
  <input
    class="form-control daterange"
    placeholder="กรุณาเลือกวันเวลาที่เปิดรับแทง"
    data-plugin-options="{&quot;timePicker&quot;: true, &quot;opens&quot;: &quot;right&quot;, &quot;timePicker24Hour&quot;: true, &quot;timePickerIncrement&quot;: 15, &quot;locale&quot;: { &quot;format&quot;: &quot;DD/MM/YYYY h:mm A&quot; }}">
</div>

<div class="form-group row">
  <label class="col-md-3 col-form-label" for="l10">ความเสี่ยงสูงสุด :</label>
  <div class="col-md-9">
    <div class="input-group">
        <%= form.number_field "rounds", id: :value, autofocus: true, placeholder: "ความเสี่ยงสูงสุด", class:"form-control", step: :any %>
    </div>
  </div>
</div>


<div class="form-group row">
  <label class="col-md-3 col-form-label" for="l10"> สามตัวบน:</label>
  <div class="col-md-3">
