        <%= form.number_field "สามตัวบน", id: :value, autofocus: true, placeholder: "750", class:"form-control", step: :any %>
    </div>
  </div>
  <div class="col-md-3">
    <div class="input-group">
        <%= form.number_field "เลขที่ออก", id: :value, autofocus: true, placeholder: "750", class:"form-control", step: :any %>
